n = 0
tab = []
res = 1
k=0
while n < 1 or n > 20 and k<2:
    n = int(input("Введите число(цикл заканчивается при вводе числа от 1 до 20): "))
    tab.append(n)
    k+= 1
for i in tab:
    res *= i
print("Среднее геометрическое:", res ** (1 / len(tab)))
